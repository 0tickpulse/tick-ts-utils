{
    "name": "tick-ts-utils",
    "version": "4.2.0",
    "description": "Bunch of utilities I personally use for TypeScript.",
    "main": "out/index.js",
    "scripts": {
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=src/",
        "build": "tsc -b",
        "watch": "tsc -b -w",
        "clean": "rm -rf out",
        "cleanWindows": "rmdir /s /q out",
        "typedoc": "pnpm exec typedoc --out docs ./src --plugin @mxssfd/typedoc-theme --theme my-theme",
        "prettier": "pnpm exec prettier -w ./src"
    },
    "author": "0TickPulse",
    "license": "AGPL-3.0-or-later",
    "devDependencies": {
        "@types/jest": "^29.4.0",
        "@types/node": "^18.14.6",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "eslint": "^8.36.0",
        "eslint-plugin-tsdoc": "^0.2.17",
        "jest": "^29.4.3",
        "prettier": "^2.8.4",
        "ts-jest": "^29.0.5",
        "typedoc": "^0.23.26",
        "typedoc-plugin-markdown": "^3.14.0",
        "typescript": "^4.9.5"
    },
    "prettier": {
        "printWidth": 150,
        "tabWidth": 4,
        "trailingComma": "all"
    },
    "optionalDependencies": {
        "zod": "^3.21.0"
    },
    "dependencies": {
        "@mxssfd/typedoc-theme": "^1.0.3",
        "emphasize": "^6.0.0",
        "stacktrace-js": "^2.0.2",
        "utils": "^0.3.1"
    }
}
